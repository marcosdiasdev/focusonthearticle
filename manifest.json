    "manifest_version" : 2,

    "name" : "Focus on the Article",
    "description" : "Hides the rest of the page so you can only see the article.",
    "version" : "1.0",

    "browser_action" : {
        "default_icon" : "icon.png",
        "default_popup" : "popup.html"
    },

    "permissions": [
        "activeTab"
    ]
}
